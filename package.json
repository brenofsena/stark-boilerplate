{
  "name": "stark-boilerplate",
  "version": "1.0.0",
  "description": "A simple boilerplate for web applications",
  "scripts": {
    "start": "node index",
    "dev:base": "webpack-dev-server --config webpack.dev.js",
    "dev": "npm run dev:base -- --progress --open",
    "build": "webpack --config webpack.prod.js",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:coveralls": "npm run test:ci && coveralls < coverage/lcov.info",
    "test:cypress": "cypress open",
    "test:cypress:run": "cypress run",
    "test:cypress:ci": "start-server-and-test dev:base http-get://localhost:3333 test:cypress:run",
    "lint:js": "eslint --ext js,jsx --fix",
    "lint:css": "stylelint './src/**/*.js'",
    "lint": "npm run lint:js && npm run lint:css",
    "check": "npm-check -u -s"
  },
  "author": "Breno Sena",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/eslint-parser": "^7.11.4",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.7.4",
    "@babel/runtime": "^7.11.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.2",
    "@testing-library/react-hooks": "^3.4.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "coveralls": "^3.1.0",
    "css-loader": "^4.2.2",
    "cypress": "^5.0.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^23.6.0",
    "eslint-plugin-react": "^7.20.6",
    "faker": "^5.1.0",
    "favicons-webpack-plugin": "^4.2.0",
    "file-loader": "^6.0.0",
    "git-commit-msg-linter": "^2.5.2",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.3",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^26.4.2",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.2.11",
    "npm-check": "^5.9.2",
    "offline-plugin": "^5.0.7",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.0.0",
    "prop-types": "^15.7.2",
    "react-svg-loader": "^3.0.3",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.3",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^5.1.2",
    "webpack-pwa-manifest": "^4.1.1"
  },
  "dependencies": {
    "@apollo/client": "^3.1.4",
    "cross-fetch": "^3.0.5",
    "dotenv": "^8.2.0",
    "dotenv-webpack": "^2.0.0",
    "express": "^4.17.1",
    "express-history-api-fallback": "^2.2.1",
    "graphql": "^15.3.0",
    "polished": "^3.6.5",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hook-form": "^6.6.0",
    "react-router-dom": "^5.2.0",
    "react-slick": "^0.27.10",
    "slick-carousel": "^1.8.1",
    "styled-components": "^5.1.1"
  }
}
